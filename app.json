{
  "expo": {
    "name": "TimeChimp",
    "slug": "timechimp",
    "description": "The ultimate time registration tool.",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "icon": "./assets/icon.png",
    "platforms": [
      "ios"
    ],
    "version": "1.0.2",
    "orientation": "portrait",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.timechimp",
      "icon": "./assets/icon.png",
      "supportsTablet": false,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan receipts."
      }
    }
  }
}
